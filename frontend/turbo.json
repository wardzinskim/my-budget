{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "^watch"
      ]
    },
    "watch": {
      "persistent": false,
      "cache": false
    }
  },
  "experimentalUI": false
}